firebase\n            .firestore()\n            .collection(target)\n            .get()\n            .then((snapshot) => {\n                const allContent = snapshot.docs.map((contentObj) => ({\n                    ...contentObj.data(),\n                    docId: contentObj.id,\n                }));\n\n                setContent(allContent);\n            })\n            .catch((error) => {\n                console.log(error.message)\n            });\n    }, []);\n\n    return { [target]: content };\n};","/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/utils/selection-filter.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/containers/browse.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/containers/profiles.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/profiles/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/profiles/styles/profiles.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/loading/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/loading/styles/loading.js",[],{"ruleId":"177","replacedBy":"178"},{"ruleId":"179","replacedBy":"180"},{"ruleId":"181","severity":1,"message":"182","line":21,"column":8,"nodeType":"183","endLine":21,"endColumn":10,"suggestions":"184"},{"ruleId":"181","severity":1,"message":"185","line":24,"column":8,"nodeType":"183","endLine":24,"endColumn":10,"suggestions":"186"},"no-native-reassign",["187"],"no-negated-in-lhs",["188"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'firebase'. Either include it or remove the dependency array.","ArrayExpression",["189"],"React Hook useEffect has missing dependencies: 'firebase' and 'target'. Either include them or remove the dependency array.",["190"],"no-global-assign","no-unsafe-negation",{"desc":"191","fix":"192"},{"desc":"193","fix":"194"},"Update the dependencies array to be: [firebase]",{"range":"195","text":"196"},"Update the dependencies array to be: [f