mponents/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/footer/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/footer/styles/footer.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/containers/footer.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/containers/faqs.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/accordion/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/accordion/styles/accordion.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/opt-form/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/opt-form/styles/opt-form.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/constants/routes.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/pages/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/pages/home.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/pages/browse.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/pages/signin.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/pages/signup.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/containers/header.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/header/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/header/styles/header.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/feature/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/feature/styles/feature.js",[],["194","195"],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/lib/firebase.prod.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/seed.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/context/firebase.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/form/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/form/styles/form.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/helpers/routes.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/hooks/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/hooks/use-auth-listener.js",["196"],"import { useState, useEffect, useContext } from 'react';\nimport { FirebaseContext } from '../context/firebase';\n\nexport default function useAuthListener() {\n    const [user, setUser] = useState(JSON.parse(localStorage.getItem('authUser')));\n\n    const { firebase } = useContext(FirebaseContext);\n\n    \n    useEffect(() =>  {\n        const listener = firebase.auth().onAuthStateChanged((authUser) => {\n            if (authUser) {\n                localStorage.setItem('authUser', JSON.stringify(authUser));\n                setUser(authUser);\n            }else {\n                localStorage.removeItem('authUser');\n                setUser(null);\n            }\n        });\n        return () => listener();\n    }, []);\n\n    return { user };\n};\n","/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/hooks/use-content.js",["197"],"import { useState, useEffect, useContext } from 'react';\nimport { FirebaseContext } from '../context/firebase';\n\nexport default function useContent(target) {\n    const [content, setContent] = useState([]);\n    const { firebase } = useContext(FirebaseContext);\n\n    useEffect(() => {\n        firebase\n            .firestore()\n            .collection(target)\n            .get()\n            .then((snapshot) => {\n                const allContent = snapshot.docs.map((contentObj) => ({\n                    ...contentObj.data(),\n                    docId: contentObj.id,\n                }));\n\n                setContent(allContent);\n            })\n            .catch((error) => {\n                console.log(error.message)\n            });\n    }, []);\n\n    return { [target]: content };\n};","/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/utils/selection-filter.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/containers/browse.js",["198"],"import React, { useState, useContext, useEffect } from 'react';\nimport Fuse from 'fuse.js';\nimport { SelectProfileContainer } from './profiles';\nimport { FirebaseContext } from '../context/firebase';\nimport { Header, Loading, Card, Player } from '../components';\nimport { FooterContainer } from './footer';\nimport * as ROUTES from '../constants/routes';\nimport logo from '../logo.svg';\n\n\nexport function BrowseContainer({ slides }) {\n    const [category, setCategory] = useState('series');\n\n    const [searchTerm, setSearchTerm] = useState('');\n    const [profile, setProfile] = useState({});\n    const [loading, setLoading] = useState(true);\n    const [slideRows, setSlideRows] = useState([]);\n\n    const { firebase } = useContext(FirebaseContext);\n    const user = firebase.auth().currentUser || {};\n\n    useEffect(() => {\n        // console.log('profile', profile)\n        setTimeout(() => {\n            setLoading(false)\n        }, 3000);\n    }, [profile.displayName]);\n\n    useEffect(() => {\n        setSlideRows(slides[category])\n    }, [slides, category]);\n\n    useEffect(() => {\n        const fuse = new Fuse(slideRows, { \n            keys: ['data.description', 'data.title', 'data.genre']\n        });\n        const results = fuse.search(searchTerm).map(({ item }) => item);\n\n        if (slideRows.length > 0 && searchTerm.length > 3 && results.length > 0){\n            setSlideRows(results);\n        } else {\n            setSlideRows(slides[category])\n        }\n\n    }, [searchTerm]);\n\n    return profile.displayName ? (\n        <>\n            {loading ? <Loading src={user.photoURL} /> : <Loading.ReleaseBody />}\n            \n            <Header src=\"joker1\" dontShowOnSmallViewPort>\n\n                <Header.Frame>\n                    <Header.Group>\n                        <Header.Logo to={ROUTES.HOME} src={logo} alt=\"Netflix\" />\n                        <Header.TextLink \n                            active={category === 'series' ? 'true' : 'false'} \n                            onClick={() => setCategory('series')}\n                        >\n                        Series\n                        </Header.TextLink>\n                        <Header.TextLink \n                            active={category === 'films' ? 'true' : 'false'}\n                            onClick={() => setCategory('films')}\n                        >\n                        Films\n                        </Header.TextLink>\n                    </Header.Group>\n\n                    <Header.Group>\n                        <Header.Search\n                            searchTerm={searchTerm}\n                            setSearchTerm={setSearchTerm}\n                        />\n\n                        <Header.Profile>\n                            <Header.Picture src={user.photoURL} />\n                            \n                            <Header.Dropdown>\n                                <Header.Group>\n                                    <Header.Picture src={user.photoURL} />\n                                    <Header.TextLink>{user.displayName}</Header.TextLink>\n                                </Header.Group>\n\n                                <Header.Group>\n                                    <Header.TextLink onClick={() => firebase.auth().signOut()}>Sign Out</Header.TextLink>\n                                </Header.Group>\n                            </Header.Dropdown>\n\n                        </Header.Profile>\n                    </Header.Group>\n                </Header.Frame>\n\n                <Header.Feature>\n                    <Header.FeatureCallOut>Watch Joker Now</Header.FeatureCallOut>\n                    <Header.Text>\n                        Forever alone in a crowd, failed comedian Arthur Fleck seeks connection as he walks the streets of Gotham\n                        City. Arthur wears two masks -- the one he paints for his day job as a clown, and the guise he projects in a\n                        futile attempt to feel like he's part of the world around him.\n                    </Header.Text>\n\n                    <Header.PlayButton>Play</Header.PlayButton>\n                </Header.Feature>\n\n            </Header>\n\n            <Card.Group>\n                {slideRows.map((slideItem) => (\n                    <Card key={`${category}-${slideItem.title.toLowerCase()}`}>\n                        <Card.Title>{slideItem.title}</Card.Title>\n                        <Card.Entities>\n                            {slideItem.data.map((item) => (\n                                <Card.Item key={item.docId} item={item}>\n                                    <Card.Image \n                                        src={`/images/${category}/${item.genre}/${item.slug}/small.jpg`}\n                                    />\n                                    <Card.Meta>\n                                        <Card.SubTitle>{item.title}</Card.SubTitle>\n                                        <Card.Text>{item.description}</Card.Text>\n                                    </Card.Meta>\n                                </Card.Item>\n                            ))}\n                        </Card.Entities>\n\n                        <Card.Feature category={category}>\n                            <Player>\n                                <Player.Button />\n                                <Player.Video src=\"/videos/bunny.mp4\"/>\n                            </Player>\n                        </Card.Feature>\n\n                    </Card>\n                ))}\n            </Card.Group>\n            <FooterContainer />\n        </> \n    ) : (\n    <SelectProfileContainer user={user} setProfile={setProfile} />\n    );\n}\n\n\n ","/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/containers/profiles.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/profiles/index.js",[],"/Users/mhdhassan/Flatiron/code/6_mod/netflix/src/components/profiles/styles/profiles.js",[],"/Users/mhdhas